
<table> 
<tr style="width: 100%">
<th>Title</th>
<th>Actions</th>
</tr>
<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td>
    <%=link_to "View", post_path(post.id) %>
    <%=link_to "Edit", edit_post_path(post.id) %>
    <%=link_to "Delete", post_path(post.id), method: "delete", "data-confirm":"Kwekkwek" %>
    </td>
  </tr>
  <%end%>
 </table>


 <%=link_to "New Post", new_post_path %> 